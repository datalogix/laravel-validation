{
    "name": "datalogix/laravel-validation",
    "description": "Laravel Validation is a package the power of Respect Validation.",
    "keywords": [
        "laravel",
        "validation",
        "respect",
        "rules"
    ],
    "homepage": "https://github.com/datalogix/laravel-validation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "respect/validation": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.5|^10.5"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\Validation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\Validation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datalogix\\Validation\\ValidationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
